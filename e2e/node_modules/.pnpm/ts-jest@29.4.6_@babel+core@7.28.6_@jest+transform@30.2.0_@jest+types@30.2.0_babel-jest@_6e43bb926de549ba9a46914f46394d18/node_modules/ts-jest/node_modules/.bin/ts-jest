#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/emir/Desktop/NestJS/sleepr/e2e/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_6e43bb926de549ba9a46914f46394d18/node_modules/ts-jest/node_modules:/home/emir/Desktop/NestJS/sleepr/e2e/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_6e43bb926de549ba9a46914f46394d18/node_modules:/home/emir/Desktop/NestJS/sleepr/e2e/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/emir/Desktop/NestJS/sleepr/e2e/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_6e43bb926de549ba9a46914f46394d18/node_modules/ts-jest/node_modules:/home/emir/Desktop/NestJS/sleepr/e2e/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_6e43bb926de549ba9a46914f46394d18/node_modules:/home/emir/Desktop/NestJS/sleepr/e2e/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
